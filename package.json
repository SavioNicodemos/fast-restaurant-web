{
  "name": "fast-restaurant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve:ui": "vite preview",
    "serve:back": "json-server --watch server.json --port 3333",
    "lint": "pnpm dlx @biomejs/biome check src",
    "lint:fix": "pnpm dlx @biomejs/biome check --apply src",
    "ts:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "cypress": "npx cypress open"
  },
  "dependencies": {
    "@unform/core": "^2.1.6",
    "@unform/web": "^2.1.6",
    "axios": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.21.3",
    "styled-components": "^6.1.8",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.5.3",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-modal": "^3.16.3",
    "@types/styled-components": "^5.1.34",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "axios-mock-adapter": "^1.22.0",
    "c8": "^9.1.0",
    "cypress": "^13.6.3",
    "jsdom": "^24.0.0",
    "json-server": "1.0.0-alpha.22",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}